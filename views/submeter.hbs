  crossorigin=""></script>


  <!-- Load Esri Leaflet Geocoder from CDN -->
  <link rel="stylesheet" href="https://unpkg.com/esri-leaflet-geocoder@2.3.2/dist/esri-leaflet-geocoder.css"
    integrity="sha512-IM3Hs+feyi40yZhDH6kV8vQMg4Fh20s9OzInIIAc4nx7aMYMfo+IenRUekoYsHZqGkREUgx0VvlEsgm7nCDW9g=="
    crossorigin="">
  <script src="https://unpkg.com/esri-leaflet-geocoder@2.3.2/dist/esri-leaflet-geocoder.js"
    integrity="sha512-8twnXcrOGP3WfMvjB0jS5pNigFuIWj4ALwWEgxhZ+mxvjF5/FBPVd5uAxqT8dd2kUmTVK9+yQJ4CmTmSg/sXAQ=="
    crossorigin=""></script>


    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>

     <script src="javascripts/submeter.js"></script>

</head>

<body>
    <div class="headerHome">

            <img src="./images/azulejo.jpg" alt="errou"/>

        <div class="login-box">
            <div class="form" id="form">
                <div class="botoes">
                    <div class="username">
                        <input type="text" id="username" placeholder="Username">
                    </div>
                    <div class="password">
                        <input type="password" id="password" placeholder="Password">
                    </div>
                    <button class="btn btn1" id="entrar" onclick="validate()">Entrar</button>
                    <button class="btn btn2" id="registo">Criar Conta</button>
                </div>
            </div>
            <div class="form1" id="form1">
                <div class="botoes">
                    <div class="dados" id="dados">
                    </div>
                    <button class="btn btn1" id="editar">Editar Perfil</button>
                    <button class="btn btn2" id="sair" onclick="logout()">Terminar Sessão</button>
                </div>
            </div>
        </div>
    </div>

    <nav>
        <ul>
            <li><a href="/">Mapa</a></li>
            <li><a href="azulejo">Lista de Azulejos</a></li>
            <li><a href="equipa">Junta-te à Equipa</a></li>
        </ul>
    </nav>

    <div class="inputs">
        <table style="width:40%" class="tabelaInput">
            <tr>
                <th>Nome do Azulejo:</th>
                <td><input type="text" id="nomeAzulejo" name="fname"></td>
            </tr>
            <tr>
                <th>Morada:</th>
                <td><input type="text" id="moradaAzulejo" name="fname"></td>
            </tr>
            <tr>
                <th>Estado do Azulejo:</th>
                <td><select id="condicao">

                    </select>
                </td>
            </tr>
            <tr>
                <th>Ano:</th>
                <td><input type="number"  id="anoAzulejo" min="-1000" max="2020" name="fname"></td>
            </tr>
            <tr>
                <th>Descrição:</th>
                <td><input type="text" id="inputDesc" name="fname"></td>
            </tr>
        </table>
        <div class="imagemInput">
                <form runat="server">
                    <input id="btnEscolherImagem" type='file' />
                    <img id="blah" alt="-"/>
                </form>
                <div id="mapSubmeter" class="mapSubmeter" > 
                </div>
        </div>
    </div>
    <div>
        <button id="submeter" class="btnInserirAzulejo">Submeter Azulejo</button>
    </div>


</body>
</html>